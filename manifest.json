{
    "manifest_version": 2,
    "name": "Harambe",
    "version": "1.0.0",
    "description": "A simple Chrome extension that replaces references to divine entities with the word \"Harambe\"",
    "browser_action": {
        "default_title": "turn on Harambe"
    },
    "icons": {
        "128": "Harambe.png"
    },
    "background": {
        "scripts": [
            "background.js",
            "jquery-3.1.0.js",
            "replaceText.js"
        ],
        "persistent:": false
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts":
    [
        {
            "matches":["http://*/*"],
            "js":[
            "background.js",
            "jquery-3.1.0.js",
            "replaceText.js"
        ]
        }
    ]
}